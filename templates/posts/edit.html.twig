
{% extends 'layouts/layout.html.twig' %}
{% block title %}Nowy post{% endblock %}
{% block content %}


<h3>Edycja posta</h3>

{{ form_start(form) }}
<div class="form-group">
    {{ form_row(form.title) }}
</div>
<div class="form-group">
    {{ form_row(form.descr) }}
</div>
<div class="form-group">
    {{ form_row(form.img) }}
</div>

<input type="submit" value="Create post" class="btn btn-outline-primary">

{% endblock %}
